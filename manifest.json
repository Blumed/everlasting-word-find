{
  "name": "Everlasting Word Find",
  "description": "Create a word find game generated by web pages",
  "version": "0.1",
  "permissions": [
    "activeTab",
    "<all_urls>",
    "storage",
    "tabs",
    "contextMenus"
  ],
  "browser_action": {
    "default_icon": "assets/images/icon-128.png"
  },
  "icons": {
    "16": "assets/images/icon-16.png",
    "48": "assets/images/icon-48.png",
    "128": "assets/images/icon-128.png"
  },
  "background": {
    "scripts": [
      "assets/js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "assets/js/vendor/jquery-3.2.1.min.js",
        "assets/js/contentScript.js",
        "assets/js/wordfind.js",
        "assets/js/wordfindgame.js"
      ],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    "index.html",
    "modal.js"
  ],
  "manifest_version": 2
}